<div class="container">
  <div class="row">
    <div class="col-xs-offset-1 col-xs-11">
      <h3>会員一覧</h3>
        <table class="table table-bordered">
        <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
        </tr>
          <% @customer.each do |customer| %>
          <tr>
              <td><%= customer.id %></td>
              <td>
                <%= link_to admin_customer_path(customer) do %>
                  <%= customer.first_name %> <%= customer.last_name %>


                <% end %>
              </td>
              <td><%= customer.email %></td>
          <% end %>
      </table>
    </div>
  </div>
</div>