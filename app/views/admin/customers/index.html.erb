<div class="container">
  <div class="row">
    <div class="col-xs-offset-1 col-xs-11">
      <h3>会員一覧</h3>
      　<table class="table table-bordered">
	  　　<tr>
	  　　    <th>会員ID</th>
	  　　    <th>氏名</th>
	  　　    <th>メールアドレス</th>
	  　　    <th>ステータス</th>
	  　　</tr>
          <% @users.each do |user| %>
          <tr>
              <td><%= user.id %></td>
              <td>
                  <%= link_to admin_user_path(user) do %>
                     <%= user.first_name %> <%= user.last_name %>
                  <% end %>
              </td>
              <td><%= user.email %></td>
          <% end %>
　　　　</table>
    </div>
  </div>
</div>